{
    "manifest_version": 3,
    "name": "Hello Extensions Testing",
    "description": "A custom new tab page extension",
    "chrome_url_overrides": {
      "newtab": "newtab.html"
    },
    "version": "1.0",
    "action": {
      "default_popup": "hello.html",
      "default_icon": "hello_extensions.png"
    },
    "permissions": ["storage", "https://api.unsplash.com/"]
}